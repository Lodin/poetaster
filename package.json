    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.6.0",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.4.2",
    "tslint-eslint-rules": "^3.4.0",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.2.0",
    "url-loader": "^0.5.7",
    "wc-loader": "^1.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
