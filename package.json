{
  "name": "poetaster",
  "version": "1.0.0-alpha.0",
  "description": "An application that helps people create poems",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "bower": "bower install && npm run patch",
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js",
    "patch": "node config/patch/patch.js",
    "polymer": "node config/prepare-polymer-typings.js",
    "postinstall": "bower install",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config=config/webpack.dev.js",
    "test": "karma start config/karma.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lodin/poetaster.git"
  },
  "keywords": [
    "poem",
    "poet",
    "helper",
    "poetaster"
  ],
  "author": "Vlad Rindevich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lodin/poetaster/issues"
  },
  "homepage": "https://github.com/Lodin/poetaster#readme",
  "devDependencies": {
    "@polymer/polymer": "git+https://github.com/Polymer/polymer.git#2.0-typescript",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.39",
    "@types/sinon": "^1.16.35",
    "@types/universal-router": "0.0.2",
    "awesome-typescript-loader": "^3.0.4",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "bower": "^1.8.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "polymer2-elements-typings": "git+https://github.com/Lodin/polymer2-elements-typings.git",
    "postcss-cssnext": "^2.9.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-import": "^9.1.0",
